{
  "name": "nextjs-blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev1": "next dev",
    "dev": "concurrently \"next dev\" \"babel -w ./src --out-dir dist --extensions .ts,.tsx\"",
    "build": "next build",
    "start": "next start",
    "typeStart": "ts-node src/index.ts",
    "typeorm:build": "rm -rf ./dist && babel  ./src --out-dir dist --extensions '.ts,.tsx'",
    "migration:run": "typeorm migration:run",
    "migration:revert": "typeorm migration:revert",
    "entity:create": "typeorm entity:create",
    "migration:create": "typeorm migration:create"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "global": "^4.4.0",
    "gray-matter": "^4.0.2",
    "lodash": "^4.17.20",
    "marked": "^1.1.1",
    "md5": "^2.3.0",
    "next": "9.5.2",
    "next-images": "^1.4.1",
    "pg": "8.2.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.25",
    "typescript": "3.9.3",
    "ua-parser-js": "^0.7.21"
  },
  "devDependencies": {
    "@babel/cli": "7.10.1",
    "@babel/plugin-proposal-decorators": "7.10.1",
    "@types/axios": "^0.14.0",
    "@types/lodash": "^4.14.161",
    "@types/marked": "^1.1.0",
    "@types/md5": "^2.2.0",
    "@types/node": "^14.6.2",
    "@types/react": "^16.9.48",
    "@types/react-dom": "^16.9.8",
    "@types/ua-parser-js": "^0.7.33",
    "concurrently": "5.2.0",
    "file-loader": "^6.0.0"
  }
}
